<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popupViewModels="clr-namespace:LauncherFinal.ViewModels.PopupViewModels"
                    xmlns:views="clr-namespace:LauncherFinal.Views"
                    xmlns:popup="clr-namespace:LauncherFinal.Views.Popup">
    
    <DataTemplate DataType="{x:Type popupViewModels:MessageViewModel}">
        <popup:MessageView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popupViewModels:DialogViewModel}">
        <popup:DialogView/>
    </DataTemplate>

</ResourceDictionary>